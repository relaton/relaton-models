class ContributorRole{
  definition {
    A description of the role of the contributor in the production of a bibliographic item.
  }
  +type: ContributorRoleType {
    definition {
      The broad class of role of the contributor. Mandatory in order to enforce orderly management
      of contributors.
    }
  }
  +description: <<Basicdoc>>BasicElement[0..*] {
    definition {
      A more detailed description of the role of the contributor.
    }
  }
}
