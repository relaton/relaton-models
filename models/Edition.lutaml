class Edition {
  definition {
    Edition of a bibliographic item.
  }

  +number: String[0..1] {
    definition {
      Number of edition.

      NOTE: The number attribute can be used to represent the numeric equivalent
      of the edition string.
    }
  }

  +content: <<Basicdoc>>BasicElement[1..*] {
    definition {
      Formatted edition string for human reading.
    }
  }
}
