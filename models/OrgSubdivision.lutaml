class OrgSubdivision {
  definition {
    Subdivision of organization associated with a bibliographic item.
  }
  +subdivision: Organization[1..*] {
    definition {
      The subdivision, modelled recursively as an organization.
    }
  }
  +type: String[0..1] {
    definition {
      The type of subdivision.
    }
  }
  +subtype: String[0..1] {
    definition {
      The subtype of subdivision. (For example, different types of technical committee.)
    }
  }
}
