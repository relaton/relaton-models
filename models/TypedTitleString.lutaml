class TypedTitleString {
  definition {
    A title of a bibliographic item, associated with a type of title.
  }
  +type: TitleType[0..1] {
    definition {
      The type for the given title of the bibliographic item.
    }
  }
  +content: <<Basicdoc>>BasicElement[0..*] {
    definition {
      The title itself.
    }
  }
}
