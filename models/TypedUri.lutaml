class TypedUri{
  definition {
    URI associated with a type. 
  }
  +type: String[0..1] {
    definition {
      The types of URI are open-ended, but include
      the IANA link relations specified in <<rfc8288>>.
    }
  }
  +language: Iso639Code[0..*] {
    definition {
      Language of string.
    } 
  }   
  +script: Iso15924Code[0..*] {
    definition {
      Script of string.
    }
  }
  +content: Uri {
    definition {
      URI.
    }
  }
}
